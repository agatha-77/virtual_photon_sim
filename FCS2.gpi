set grid
set key top right
set key box
set key height 1
set key width 1
unset key

set logscale y

set xlabel font ",15"
set ylabel font ",15"
set xtics font ",12"
set ytics font ",12"

set ylabel offset -1

set xlabel "W_{{/Symbol g}{/Symbol g}}[GeV]"
set ylabel "{/Symbol s}_{{/Symbol g}{/Symbol g} -> e^+ e^-}[fm]"

plot 'data/fundamental_electron_CS.dat' using 1:2 w lines lw 2 linecolor 8

pause -1
